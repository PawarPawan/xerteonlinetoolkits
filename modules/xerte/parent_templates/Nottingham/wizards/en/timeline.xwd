<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>      <timeline><![CDATA[<timeline name="Enter Page Title" text="Enter text for the page here" interactivity="Timeline" feedback="Enter feedback here" checkBtnTxt="Check Answers" checkBtnTip="Check Answers" checkBtnWidth="100"/>]]></timeline>    </newNodes>  </pageWizard>  <!-- TIMELINE INTERACTION========================= -->  <timeline menu="Interactivity" menuItem="Timeline / Matching Pairs" hint="An interaction for creating an interactive timeline." icon="icTime" thumb="thumbs/timeline.jpg" remove="true">    <name label="Page Title" type="TextInput"/>        <text label="Text" type="TextArea" height="250"/>    <interactivity label="Interactivity" options="Timeline,Matching Pairs" type="ComboBox" data="Timeline,Matching Pairs" defaultValue="Timeline" width="120"/>    <feedback label="Feedback" type="TextArea" height="100"/>    <checkBtnTxt label="Check Button Label" type="textInput" />    <checkBtnTip label="Check Button Tip" type="textInput" />	    <checkBtnWidth label="Check Button Width" type="NumericStepper" min="100" max="250" step="10" width="100" defaultValue="100" />	    <narrationNavigate label="Navigate on narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />    <newNodes>      <timelinedate><![CDATA[<timelinedate name="Enter Item" text="Enter description"/>]]></timelinedate>    </newNodes>  </timeline>  <timelinedate menuItem="Item" icon="icPageWhite" max="12" remove="true">    <name label="Target" type="TextInput"/>        <text label="Label" type="TextArea" height="100"/>  </timelinedate></wizard>