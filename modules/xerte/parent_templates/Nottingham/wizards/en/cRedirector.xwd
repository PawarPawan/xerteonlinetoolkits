<wizard menus="Connectors">  <pageWizard remove="true" duplicate="false">    <newNodes>        <cRedirector><![CDATA[<cRedirector name="Enter Page Title" pageID="Enter a unique ID so that Connector Pages can find this page" destination="Page ID of page to redirect to." endText="You have completed this section."	  textWidth="narrow" align="Left" pageMode="development" exitNav="prior" nextSet="1" backSet="1" progSet="1" tocSet="1" continueBtnTxt="Continue" continueBtnTip="Click to Continue" continueBtnWidth="100"	  reportTitle="List of page titles and matching pageIDs" pageLabel="Page No" titleLabel="Title:" idLabel="pageID:" modeLabel="Mode"	  notFoundMessage="could not be found in this project." notSetMessage="A destination page has not been set for this Connection"/>]]></cRedirector>	  </newNodes>  </pageWizard>  <!-- TEXT PAGE =================================== -->  <cRedirector menu="Connectors" menuItem="Redirector Connector"  hint="This page redirects the project flow to a specified page, use it at the end of a sequence of none connector pages to return to the main flow of the learning object." thumb="thumbs/connectorRedirector.jpg"  remove="true">    <name label="Page Title" type="TextInput" />    <pageID label="Page ID" type="TextInput" />    <destination label="Destination Page ID" type="TextInput" />    <endText label="Instructions / Message" type="textArea" height="200"/>    <textWidth label="Min Text Width" options="Narrow,Wide,Max Available" type="ComboBox"  data="narrow,wide,max" defaultValue="narrow" width="200"/>		    <align label="Align Text" options="Left,Right" type="ComboBox"  data="Left,Right" defaultValue="Left" width="200" />	    <pageMode label="Mode" options="Development,Live" type="ComboBox" data="development,live" defaultValue="development" width="200"/>    <exitNav label="Navigation on Exit" options="As for previous page,Show all navigation buttons" type="ComboBox" data="prior,all" defaultValue="prior" width="200"/>    <nextSet label="Next / Continue Button" options="Show Next and Continue Buttons,Hide Next and Continue Buttons" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <backSet label="Back Button" options="Show Back Button,Hide Back Button" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <progSet label="Progress Button" options="Show Progress Button,Hide Progress Button" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <tocSet label="Contents Button" options="Show Table of Contents,Hide Table of Contents" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <continueBtnTxt label="Continue Button Label" type="textInput"  divider="true" language="true"/>    <continueBtnTip label="Continue Button Tip" type="textInput"  language="true"/>	    <continueBtnWidth label="Continue Button Width" type="NumericStepper" min="100" max="250" step="10" width="100" defaultValue="100"  language="true"/>    <reportTitle label="Report Title" type="TextInput" language="true"/>    <pageLabel label="Page Label" type="TextInput" language="true"/>    <titleLabel label="Title Label" type="TextInput" language="true"/>	    <idLabel label="PageID Label" type="TextInput" language="true"/>    <modeLabel label="Mode Label" type="TextInput" language="true"/>	    <notFoundMessage label="Not Found Message" type="TextInput" language="true"/>    <notSetMessage label="Not Set Message" type="TextInput" language="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>	    <url label="Image" type="media" optional="true"/>    <tip label="Image Tooltip" type="TextInput" optional="true"/>		    <listToClip label="Output Check List" options="Don't send Page Title and IDs data to clipboard,Send Text Title and IDs List to Clipboard (Developer mode only),Send HTML Title and IDs Table to Clipboard (Developer mode only)" type="ComboBox" data="no,text,html" defaultValue="html" width="380" optional="true"/>  </cRedirector></wizard>