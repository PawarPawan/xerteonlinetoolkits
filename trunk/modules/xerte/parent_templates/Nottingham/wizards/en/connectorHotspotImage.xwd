<wizard menus="Connectors">  <pageWizard remove="true" duplicate="false">    <newNodes>	  <connectorHotspotImage><![CDATA[<connectorHotspotImage name="Enter Page Name" pageID="Unique ID for this page" text="Enter text for the page here" textWidth="narrow" url="Select an image" tip="Enter a tooltip for the image" hidePanel="false" highlight="false" hicol="0xFFFF00" align="Left" pageMode="development" nextSet="0" backSet="0" progSet="0" tocSet="0" />]]></connectorHotspotImage>	</newNodes>    </pageWizard>  <!-- HOTSPOT IMAGE INTERACTION ================================= -->  <connectorHotspotImage  menu="Connectors" menuItem="Hotspot Image Connector" hint="An interaction allowing you to mark up an image with regions and associate a destination page with each region." icon="icPageWhiteText" thumb="thumbs/connectorHotspotImage.jpg"  remove="true">    <name label="Page Title" type="TextInput" />    <pageID label="Page ID" defaultValue="Enter a unique ID so that Connector Pages can find this page" type="TextInput" />	    <text label="Text" type="TextArea" height="200"/>    <textWidth label="Min Text Width" options="No Text,Narrow,Wide,Max Available" type="ComboBox"  data="none,narrow,wide,max" defaultValue="narrow" width="200"/>		    <url label="Image" type="media"/>    <tip  label="Image Tooltip" type="TextInput"/>    <hidePanel label="Hide panel" type="CheckBox" defaultValue="false"/>	    <highlight label="Highlight Hotspots" type="CheckBox" defaultValue="false"/>    <hicol label="Highlight Colour" type="ColourPicker" defaultValue="0xFFFF00"/>     <align label="Align Text" options="Left,Right" type="ComboBox" data="Left,Right" defaultValue="Left" width="100"/>    <pageMode label="Mode" options="Development,Live" type="ComboBox" data="development,live" defaultValue="development" width="100"/>    <nextSet label="Next / Continue Button" options="Show Next and Continue Buttons,Hide Next and Continue Buttons" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <backSet label="Back Button" options="Show Back Button,Hide Back Button" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <progSet label="Progress Button" options="Show Progress Button,Hide Progress Button" type="ComboBox" data="1,0" defaultValue="0" width="200"/>    <tocSet label="Contents Button" options="Show Table of Contents,Hide Table of Contents" type="ComboBox" data="1,0" defaultValue="0" width="200"/>	    <imageTransition label="Image transition" options="blinds,dissolve,fade,fly,iris,squeeze,wipe,zoom" type="ComboBox" data="blinds,dissolve,fade,fly,iris,squeeze,wipe,zoom" defaultValue="wipe" width="100" optional="true"/>    <listToClip label="Output Check List" options="Do not send Page Title and IDs data to clipboard,Send Text Title and IDs List to Clipboard (Developer mode only),Send HTML Title and IDs Table to Clipboard (Developer mode only)" type="ComboBox" data="no,text,html" defaultValue="html" width="380" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <newNodes>      <cHotspot><![CDATA[<cHotspot name="name" destination="Enter Page ID of page to go to" text="Description for the hotspot" x="0" y="0" w="100" h="100" />]]></cHotspot>      <cHotspotLang><![CDATA[<cHotspotLang name="Display text setup" reportTitle="List of page titles and matching pageIDs" pageLabel="Page No" titleLabel="Title:" idLabel="pageID:" modeLabel="Mode" notFoundMessage="could not be found in this project." notSetMessage="A destination page has not been set for this Connection" continueBtnTxt="Close" continueBtnWidth="100" />]]></cHotspotLang>    </newNodes>  </connectorHotspotImage>  <!-- cHOTSPOT =============================================== -->  <cHotspot menuItem="HotSpot" label="Edit Hotspot" type="hotSpot"  icon="icLayers" remove="true">    <name label="Hotspot Title" type="TextInput"/>	<destination label="Go to Page ID" type="TextInput" optional="true"/>    <nextSet label="Next / Continue Button" options="Show Next and Continue Buttons,Hide Next and Continue Buttons" type="ComboBox" data="1,0" defaultValue="0" width="200" optional="true"/>    <backSet label="Back Button" options="Show Back Button,Hide Back Button" type="ComboBox" data="1,0" defaultValue="0" width="200" optional="true"/>    <progSet label="Progress Button" options="Show Progress Button,Hide Progress Button" type="ComboBox" data="1,0" defaultValue="0" width="200" optional="true"/>    <tocSet label="Contents Button" options="Show Table of Contents,Hide Table of Contents" type="ComboBox" data="1,0" defaultValue="0" width="200" optional="true"/>    <hotspotSound label="Sound" type="media" optional="true"/>	<hotspotMovie label="Video" type="media" optional="true"/>    <startFrame label="Video Start Time" type="textInput" defaultValue="0" optional="true"/>    <endFrame label="Video End Time" type="textInput" defaultValue="0" optional="true"/>    <movieSize label="Video Size (w,h)" type="textInput" defaultValue="320,240" optional="true"/>    <tip label="Video Tooltip" type="TextInput" optional="true"/>	    <reset label="Video Reset at End" type="Checkbox" defaultValue="false" optional="true"/>		<windowURL label="URL of External Page" type="TextInput" optional="true"/>    <windowWidth label="URL Window Width" type="NumericStepper" min="100" max="980" step="10" defaultValue="550" width="100" optional="true"/>    <windowHeight label="URL Window Height" type="NumericStepper" min="100" max="720" step="10" defaultValue="400" width="100" optional="true"/>    <htm label="URL HTML" type="textArea" height="150" optional="true"/>	    <relNav label="Relative Connection" options="First Page,Previous Page,Next Page,Last Page" type="ComboBox" data="first,prev,next,last" defaultValue="next" width="200" optional="true"/>    <hotspotPopUp label="Text Pop-up" type="TextArea" height="200" optional="true"/>	  </cHotspot>  <!-- HOTSPOTLANG =============================================== -->  <cHotspotLang menuItem="Display Text" icon="icLayers" max="1" remove="true">    <name label="Display Text" type="TextInput"/>    <reportTitle label="Report Title" type="TextInput"/>    <pageLabel label="Page Label" type="TextInput"/>    <titleLabel label="Title Label" type="TextInput"/>	    <idLabel label="PageID Label" type="TextInput"/>    <modeLabel label="Mode Label" type="TextInput"/>	    <notFoundMessage label="Not Found Message" type="TextInput"/>    <notSetMessage label="Not Set Message" type="TextInput"/>    <continueBtnTxt label="Close Button Label" type="TextInput"/>    <continueBtnWidth label="Close Button Width" type="NumericStepper" min="80" max="150" step="10" width="100" defaultValue="100" />			  </cHotspotLang>  </wizard>